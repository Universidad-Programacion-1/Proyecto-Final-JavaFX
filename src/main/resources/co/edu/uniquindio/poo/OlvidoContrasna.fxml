<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="284.0" prefWidth="356.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.viewController.OlvidoContrasnaViewController">
   <children>
      <Label fx:id="txtTitleVehiculos" graphicTextGap="5.0" prefHeight="30.0" prefWidth="200.0" style="-: center;" text="Olvido la contraseña" textAlignment="RIGHT" textOverrun="CENTER_ELLIPSIS">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Pane prefHeight="208.0" prefWidth="316.0">
         <children>
            <TextField fx:id="txtOlvidoPalabraSecreta" layoutX="128.0" layoutY="23.0" promptText="Ingrese la palabra secreta" />
            <Label layoutX="1.0" layoutY="23.0" prefHeight="25.0" prefWidth="118.0" text="Palabra Secreta:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button fx:id="btnOlvidoContrasena" layoutX="112.0" layoutY="91.0" mnemonicParsing="false" onAction="#onIniciarPalabra" prefHeight="25.0" prefWidth="93.0" text="Iniciar sesión" />
            <Label fx:id="txtOlvidoContrasena" layoutX="84.0" layoutY="125.0" onDragDetected="#onProbarCorreo" prefHeight="25.0" prefWidth="149.0" text="Probar de otra forma">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
